{
    "swagger": "2.0",
    "info": {
      "title": "API da Minha Aplicação Flask",
      "description": "Uma API de exemplo criada com Flask e Swagger",
      "version": "1.0"
    },
    "basePath": "/",
    "schemes": ["http"],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "paths": {
      "/": {
        "get": {
          "summary": "Obter uma saudação",
          "description": "Retorna uma mensagem de saudação.",
          "responses": {
            "200": {
              "description": "Mensagem de saudação retornada com sucesso.",
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  